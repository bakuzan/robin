{
  "name": "robin",
  "version": "1.15.0",
  "scripts": {
    "ng": "ng",
    "server": "nodemon apollo-server",
    "start": "npm-run-all -p serve server",
    "serve": " ng serve --aot",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:all": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.10",
    "@angular/common": "^8.2.10",
    "@angular/compiler": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/forms": "^8.2.10",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.10",
    "@angular/platform-browser-dynamic": "^8.2.10",
    "@angular/pwa": "^0.803.9",
    "@angular/router": "^8.2.10",
    "@angular/service-worker": "^8.2.10",
    "@swimlane/ngx-charts": "^12.0.1",
    "apollo-server": "^2.9.6",
    "classnames": "^2.2.6",
    "core-js": "^2.5.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "husky": "^3.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "rxjs": "~6.5.2",
    "sequelize": "^5.19.6",
    "sequelize-cli": "^5.5.1",
    "sqlite": "^3.0.3",
    "umzug": "^2.2.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "^8.2.10",
    "@angular/language-service": "^8.2.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "~3.5.3"
  }
}
